<tr role="row" class="odd">
    <td class="w-50" tabindex="0">{{index}}</td>
    {{#enrolledStudents}}
        <td class="wdm_course_name has-student" data-courseid="{{id}}"><a href="javascript:void(0)">{{{fullname}}}</a></td>
    {{/enrolledStudents}}
    {{^enrolledStudents}}
        <td class="wdm_course_name" data-courseid="{{id}}" >{{{fullname}}}</td>
    {{/enrolledStudents}}
    <td>{{startdate}}</td>
    <td class="w-100"><span class="w-full pl-40">{{enrolledStudents}}</span></td>
    <td class=" w-100 px-10">
        <div class="pie-progress pie-progress-xs m-0 w-35" data-plugin="pieProgress" data-valuemax="50" data-barcolor="#11c26d" data-size="20" data-barsize="3" data-goal="35" aria-valuenow="{{percentage}}" role="progressbar" style="max-width: 35px!important;">
        <div class="pie-progress-content" style="z-index:2;">
        </div>
        <span class=" progress-percent" style="margin-left: 50px;position: absolute;top: 8px;">{{percentage}}%</span>
        </div>
    </td>
</tr>

