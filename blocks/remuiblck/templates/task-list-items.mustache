{{!
    @template block_timeline/event-list-item

    This template renders an event list item loading placeholder for the timeline block.

    Example context (json):
    {}
}}
{{#search}}
<label class="py-5 m-0">{{#str}} searchresultfor, block_remuiblck, {{search}}{{/str}} <a data-region="cancel-result" href="#" class="px-5">x</a></label>
{{/search}}
{{#tasks}}
    {{> block_remuiblck/task-list-item }}
{{/tasks}}
{{^tasks}}
    <div class="no-tasks m-auto text-center">
        <img src="{{no_tasks_image}}">
        {{#str}} notasks, block_remuiblck {{/str}}
    </div>
{{/tasks}}
