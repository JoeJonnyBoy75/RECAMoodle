{{!
    Page navbar.
}}
<nav class="fixed-top navbar navbar-light {{#navbarinverse}}nav-inverse{{/navbarinverse}}  navbar-expand moodle-has-zindex p-0" aria-label="{{#str}}sitemenubar, admin{{/str}}">
    <div class="navbar-brand navbar-brand-center p-0 m-0 h-100">
        {{# output.should_display_logo }}
        {{# isiconsitename }}
            <a class="text-white d-flex align-items-center justify-content-center h-100 w-full text-decoration-none" href="{{{ config.wwwroot }}}">
                <span class="navbar-brand-logo mini"><i class="fa fa-{{siteicon}}"></i></span>
                <span class="navbar-brand-logo font-weight-bold">
                    <i class="fa fa-{{siteicon}}"></i>&nbsp;
                    {{{ sitename }}}
                </span>
            </a>
        {{/ isiconsitename }}
            
        {{# islogo }}
            <a class="text-white d-flex align-items-center justify-content-center h-100 w-full text-decoration-none" href="{{{ config.wwwroot }}}">
                <span class="navbar-brand-logo mini" style="background-image: url({{ logominiurl }});
                    background-position: center; background-size: contain; background-repeat: no-repeat;"></span>
                <span class="navbar-brand-logo" style="background-image: url({{ logourl }});
                    background-position: center; background-size: contain; background-repeat: no-repeat;">
                </span>
            </a>
        {{/ islogo }}
        {{/ output.should_display_logo }}
    </div>
    <div data-region="drawer-toggle" class="d-inline-block m-0 h-100">
        <button aria-expanded="{{#navdraweropen}}true{{/navdraweropen}}{{^navdraweropen}}false{{/navdraweropen}}" aria-controls="nav-drawer" type="button" class="btn nav-link float-sm-left  bg-transparent" data-action="toggle-drawer" data-side="left" data-preference="drawer-open-nav">{{#pix}}i/menubars{{/pix}}<span class="sr-only">{{#str}}sidepanel, core{{/str}}</span></button>
    </div>
    <div class="nav-item" id="toggleFullscreen" title="{{#str}}togglefullscreen, theme_remui{{/str}}">
        <a class="nav-link icon m-0 px-3" data-toggle="fullscreen" href="#" role="button">
            {{! Expand Icon SVG }}
            <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="expand" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512" class="svg-inline--fa fa-expand fa-w-14 fa-3x"><path fill="currentColor" d="M0 180V56c0-13.3 10.7-24 24-24h124c6.6 0 12 5.4 12 12v40c0 6.6-5.4 12-12 12H64v84c0 6.6-5.4 12-12 12H12c-6.6 0-12-5.4-12-12zM288 44v40c0 6.6 5.4 12 12 12h84v84c0 6.6 5.4 12 12 12h40c6.6 0 12-5.4 12-12V56c0-13.3-10.7-24-24-24H300c-6.6 0-12 5.4-12 12zm148 276h-40c-6.6 0-12 5.4-12 12v84h-84c-6.6 0-12 5.4-12 12v40c0 6.6 5.4 12 12 12h124c13.3 0 24-10.7 24-24V332c0-6.6-5.4-12-12-12zM160 468v-40c0-6.6-5.4-12-12-12H64v-84c0-6.6-5.4-12-12-12H12c-6.6 0-12 5.4-12 12v124c0 13.3 10.7 24 24 24h124c6.6 0 12-5.4 12-12z" class=""></path></svg>
            {{! Collapse Icon SVG }}
            <svg aria-hidden="true" focusable="false" data-prefix="fas" data-icon="compress" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512" class="svg-inline--fa fa-compress fa-w-14 fa-3x"><path fill="currentColor" d="M436 192H312c-13.3 0-24-10.7-24-24V44c0-6.6 5.4-12 12-12h40c6.6 0 12 5.4 12 12v84h84c6.6 0 12 5.4 12 12v40c0 6.6-5.4 12-12 12zm-276-24V44c0-6.6-5.4-12-12-12h-40c-6.6 0-12 5.4-12 12v84H12c-6.6 0-12 5.4-12 12v40c0 6.6 5.4 12 12 12h124c13.3 0 24-10.7 24-24zm0 300V344c0-13.3-10.7-24-24-24H12c-6.6 0-12 5.4-12 12v40c0 6.6 5.4 12 12 12h84v84c0 6.6 5.4 12 12 12h40c6.6 0 12-5.4 12-12zm192 0v-84h84c6.6 0 12-5.4 12-12v-40c0-6.6-5.4-12-12-12H312c-13.3 0-24 10.7-24 24v124c0 6.6 5.4 12 12 12h40c6.6 0 12-5.4 12-12z" class=""></path></svg>
        </a>
    </div>
    {{#hasrecent}}
        <div class="nav-item dropdown " id="recentCourses">
            <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="javascript:void(0)" data-animation="scale-up" aria-expanded="true" role="button">{{#str}}recent, theme_remui{{/str}}</a>
            <div class="dropdown-menu" role="menu">
                {{#recentcourses}}
                <a class="dropdown-item" role="menuitem" href="{{{ config.wwwroot }}}/course/view.php?id={{{id}}}">{{{fullname}}}</a>
                {{/recentcourses}}
            </div>
        </div>
    {{/hasrecent}}
    <ul class="navbar-nav d-none d-md-flex h-100 wdm-custom-menus">
        <!-- custom_menu -->
        {{{ output.custom_menu }}}
        <!-- page_heading_menu -->
        {{{ output.page_heading_menu }}}
        <li class="nav-item dropdown wdm-ofdd">
            <a class="dropdown-toggle nav-link" id="ofmenu" data-toggle="dropdown">
                <span><i class="fa fa-ellipsis-h" aria-hidden="true"></i></span>
            </a>
            <div class="dropdown-menu ofdropdownmenu" aria-labelledby="ofmenu">
            </div>
        </li>
    </ul>
    <button type="button" class="navbar-toggler">
        <i class="icon fa fa-ellipsis-v" aria-hidden="true"></i>
    </button>
    <ul class="nav navbar-nav ml-auto mr-2 h-100 right-menu">
        <div class="d-none d-lg-block">
        {{{ output.search_box }}}
        </div>
        <!-- navbar_plugin_output : 'message', 'notifications' and 'chat sidebar' toggles -->
        <li class="nav-item">
        {{{ output.navbar_plugin_output }}}
        </li>
        <!-- user_menu -->
        <li class="nav-item d-flex align-items-center">
            {{{ output.user_menu }}}
        </li>
        {{# cansignup }}
        <li class="nav-item d-flex align-items-center">
            <a href="{{signupurl}}" class="nav-link"><i class="icon fa fa-user-plus"></i> {{#str}}startsignup{{/str}}</a>
        </li>
        {{/ cansignup }}
    </ul>
    <!-- search_box -->
</nav>
{{{ output.show_license_notice }}}

